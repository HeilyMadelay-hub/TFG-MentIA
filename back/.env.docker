# Configuración específica para Docker
# Este archivo sobrescribe las configuraciones del .env cuando se ejecuta con Docker

# ChromaDB en Docker
CHROMA_HOST=chromadb
CHROMA_PORT=8000

# Frontend URL en Docker
FRONTEND_URL=http://frontend

# CORS para Docker
CORS_ORIGINS=http://localhost,http://frontend,http://localhost:80

# Docker Environment
DOCKER_ENV=true
